#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00FB31D0 .scope module, "initial_fork_join" "initial_fork_join" 2 1;
 .timescale 0 0;
v00FB5168_0 .var "clk", 0 0;
v00FB3258_0 .var "data", 0 0;
v00FB32B0_0 .var "enable", 0 0;
v00FBEDE0_0 .var "reset", 0 0;
    .scope S_00FB31D0;
T_0 ;
    %vpi_call 2 5 "$monitor", "%g clk=%b reset=%b enable=%b data=%b", $time, v00FB5168_0, v00FBEDE0_0, v00FB32B0_0, v00FB3258_0;
    %fork t_1, S_00FB31D0;
    %fork t_2, S_00FB31D0;
    %fork t_3, S_00FB31D0;
    %delay 3, 0;
    %set/v v00FB3258_0, 0, 1;
    %join;
    %join;
    %join;
    %jmp t_0;
t_1 ;
    %delay 1, 0;
    %set/v v00FB5168_0, 0, 1;
    %end;
t_2 ;
    %delay 10, 0;
    %set/v v00FBEDE0_0, 0, 1;
    %end;
t_3 ;
    %delay 5, 0;
    %set/v v00FB32B0_0, 0, 1;
    %end;
t_0 ;
    %delay 1, 0;
    %vpi_call 2 13 "$display", "%g Terminating simulation", $time;
    %vpi_call 2 14 "$finish";
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 3;
    "N/A";
    "<interactive>";
    "initial_fork_join.v";
