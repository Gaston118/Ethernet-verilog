#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_010A8BB8 .scope module, "lfsr_galois_tb1" "lfsr_galois_tb1" 2 9;
 .timescale -9 -12;
v00F71A90_0 .var "clk", 0 0;
v00F7FA60_0 .var/i "i", 31 0;
v00F7FAB8_0 .var "i_rst", 0 0;
v00F7FB10_0 .var "i_seed", 7 0;
v00F7FC40_0 .var "i_soft_reset", 0 0;
v00F80110_0 .var "i_valid", 0 0;
S_010A8910 .scope task, "async_reset" "async_reset" 2 57, 2 57, S_010A8BB8;
 .timescale -9 -12;
v00F71A38_0 .var "random_reset", 7 0;
TD_lfsr_galois_tb1.async_reset ;
    %vpi_func 2 60 "$urandom", 8, 32;
    %movi 40, 100, 32;
    %mod 8, 40, 32;
    %ix/load 0, 8, 0;
    %assign/v0 v00F71A38_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 62 "$display", "Numero: %0d", v00F71A38_0;
    %set/v v00F7FAB8_0, 1, 1;
    %load/v 8, v00F71A38_0, 8;
    %movi 16, 0, 11;
    %muli 8, 1000, 19;
    %ix/get 0, 8, 19;
    %delayx 0;
    %set/v v00F7FAB8_0, 0, 1;
    %end;
S_010A7EF8 .scope task, "change_seed" "change_seed" 2 81, 2 81, S_010A8BB8;
 .timescale -9 -12;
v00F719E0_0 .var "seed_value", 7 0;
TD_lfsr_galois_tb1.change_seed ;
    %load/v 8, v00F719E0_0, 8;
    %set/v v00F7FB10_0, 8, 8;
    %delay 10000, 0;
    %end;
S_010A8228 .scope task, "random_valid" "random_valid" 2 89, 2 89, S_010A8BB8;
 .timescale -9 -12;
TD_lfsr_galois_tb1.random_valid ;
    %set/v v00F7FA60_0, 0, 32;
T_2.0 ;
    %load/v 8, v00F7FA60_0, 32;
   %cmpi/s 8, 100, 32;
    %jmp/0xz T_2.1, 5;
    %vpi_func 2 92 "$random", 8, 32;
    %movi 40, 2, 32;
    %mod/s 8, 40, 32;
    %set/v v00F80110_0, 8, 1;
    %delay 100000, 0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v00F7FA60_0, 32;
    %set/v v00F7FA60_0, 8, 32;
    %jmp T_2.0;
T_2.1 ;
    %end;
S_010A81A0 .scope task, "soft_reset" "soft_reset" 2 69, 2 69, S_010A8BB8;
 .timescale -9 -12;
v00F71988_0 .var "random_soft_reset", 7 0;
TD_lfsr_galois_tb1.soft_reset ;
    %vpi_func 2 72 "$urandom", 8, 32;
    %movi 40, 100, 32;
    %mod 8, 40, 32;
    %ix/load 0, 8, 0;
    %assign/v0 v00F71988_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 74 "$display", "Numero: %0d", v00F71988_0;
    %set/v v00F7FC40_0, 1, 1;
    %load/v 8, v00F71988_0, 8;
    %movi 16, 0, 11;
    %muli 8, 1000, 19;
    %ix/get 0, 8, 19;
    %delayx 0;
    %set/v v00F7FC40_0, 0, 1;
    %end;
S_010A84D0 .scope module, "dut" "lfsr_galois" 2 18, 3 1, S_010A8BB8;
 .timescale -9 -12;
L_010AC948 .functor BUFZ 8, v010A32A0_0, C4<00000000>, C4<00000000>, C4<00000000>;
v010AB8A0_0 .net "clk", 0 0, v00F71A90_0; 1 drivers
v010A7A08_0 .net "feedback", 0 0, L_00F80168; 1 drivers
v00F82458_0 .net "i_rst", 0 0, v00F7FAB8_0; 1 drivers
v00F824B0_0 .net "i_seed", 7 0, v00F7FB10_0; 1 drivers
v00F82508_0 .net "i_soft_reset", 0 0, v00F7FC40_0; 1 drivers
v010A3248_0 .net "i_valid", 0 0, v00F80110_0; 1 drivers
v010A32A0_0 .var "lfsr_reg", 7 0;
v010A32F8_0 .net "o_lfsr", 7 0, L_010AC948; 1 drivers
v00F71930_0 .var "seed_reg", 7 0;
E_010AA8D8 .event posedge, v00F82458_0, v010AB8A0_0;
L_00F80168 .part v010A32A0_0, 7, 1;
    .scope S_010A84D0;
T_4 ;
    %movi 8, 1, 8;
    %set/v v00F71930_0, 8, 8;
    %end;
    .thread T_4;
    .scope S_010A84D0;
T_5 ;
    %wait E_010AA8D8;
    %load/v 8, v00F82458_0, 1;
    %jmp/0xz  T_5.0, 8;
    %load/v 8, v00F71930_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v010A32A0_0, 0, 8;
    %jmp T_5.1;
T_5.0 ;
    %load/v 8, v00F82508_0, 1;
    %jmp/0xz  T_5.2, 8;
    %load/v 8, v00F824B0_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v010A32A0_0, 0, 8;
    %jmp T_5.3;
T_5.2 ;
    %load/v 8, v010A3248_0, 1;
    %jmp/0xz  T_5.4, 8;
    %load/v 8, v010A7A08_0, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 0, 0;
    %assign/v0/x1 v010A32A0_0, 0, 8;
    %load/v 8, v010A32A0_0, 1; Only need 1 of 8 bits
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %ix/load 1, 1, 0;
    %assign/v0/x1 v010A32A0_0, 0, 8;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_5.6, 4;
    %load/x1p 8, v010A32A0_0, 1;
    %jmp T_5.7;
T_5.6 ;
    %mov 8, 2, 1;
T_5.7 ;
; Save base=8 wid=1 in lookaside.
    %load/v 9, v010A7A08_0, 1;
    %xor 8, 9, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 2, 0;
    %assign/v0/x1 v010A32A0_0, 0, 8;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_5.8, 4;
    %load/x1p 8, v010A32A0_0, 1;
    %jmp T_5.9;
T_5.8 ;
    %mov 8, 2, 1;
T_5.9 ;
; Save base=8 wid=1 in lookaside.
    %load/v 9, v010A7A08_0, 1;
    %xor 8, 9, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 3, 0;
    %assign/v0/x1 v010A32A0_0, 0, 8;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_5.10, 4;
    %load/x1p 8, v010A32A0_0, 1;
    %jmp T_5.11;
T_5.10 ;
    %mov 8, 2, 1;
T_5.11 ;
; Save base=8 wid=1 in lookaside.
    %load/v 9, v010A7A08_0, 1;
    %xor 8, 9, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 4, 0;
    %assign/v0/x1 v010A32A0_0, 0, 8;
    %ix/load 1, 4, 0;
    %mov 4, 0, 1;
    %jmp/1 T_5.12, 4;
    %load/x1p 8, v010A32A0_0, 1;
    %jmp T_5.13;
T_5.12 ;
    %mov 8, 2, 1;
T_5.13 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %ix/load 1, 5, 0;
    %assign/v0/x1 v010A32A0_0, 0, 8;
    %ix/load 1, 5, 0;
    %mov 4, 0, 1;
    %jmp/1 T_5.14, 4;
    %load/x1p 8, v010A32A0_0, 1;
    %jmp T_5.15;
T_5.14 ;
    %mov 8, 2, 1;
T_5.15 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %ix/load 1, 6, 0;
    %assign/v0/x1 v010A32A0_0, 0, 8;
    %ix/load 1, 6, 0;
    %mov 4, 0, 1;
    %jmp/1 T_5.16, 4;
    %load/x1p 8, v010A32A0_0, 1;
    %jmp T_5.17;
T_5.16 ;
    %mov 8, 2, 1;
T_5.17 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %ix/load 1, 7, 0;
    %assign/v0/x1 v010A32A0_0, 0, 8;
T_5.4 ;
T_5.3 ;
T_5.1 ;
    %jmp T_5;
    .thread T_5;
    .scope S_010A8BB8;
T_6 ;
    %delay 50000, 0;
    %load/v 8, v00F71A90_0, 1;
    %inv 8, 1;
    %set/v v00F71A90_0, 8, 1;
    %jmp T_6;
    .thread T_6;
    .scope S_010A8BB8;
T_7 ;
    %vpi_call 2 31 "$dumpfile", "tb/lfsr_galois_tb1.vcd";
    %vpi_call 2 32 "$dumpvars", 1'sb0, S_010A8BB8;
    %set/v v00F71A90_0, 0, 1;
    %set/v v00F80110_0, 0, 1;
    %set/v v00F7FAB8_0, 0, 1;
    %set/v v00F7FC40_0, 0, 1;
    %set/v v00F7FB10_0, 0, 8;
    %fork TD_lfsr_galois_tb1.async_reset, S_010A8910;
    %join;
    %delay 1000000, 0;
    %movi 8, 170, 8;
    %set/v v00F719E0_0, 8, 8;
    %fork TD_lfsr_galois_tb1.change_seed, S_010A7EF8;
    %join;
    %fork TD_lfsr_galois_tb1.soft_reset, S_010A81A0;
    %join;
    %delay 1000000, 0;
    %fork TD_lfsr_galois_tb1.random_valid, S_010A8228;
    %join;
    %delay 1000000, 0;
    %vpi_call 2 54 "$finish";
    %end;
    .thread T_7;
