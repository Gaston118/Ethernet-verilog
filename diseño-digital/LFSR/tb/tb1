S_001F8720 .scope task, "async_reset" "async_reset" 2 58, 2 58, S_001F8610;
 .timescale -9 -12;
v009110A8_0 .var "random_reset", 7 0;
TD_lfsr_galois_tb1.async_reset ;
    %vpi_func 2 61 "$urandom_range", 8, 32, 2'sb01, 12'sb011111010000;
    %ix/load 0, 8, 0;
    %assign/v0 v009110A8_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 63 "$display", "Numero: %0d", v009110A8_0;
    %set/v v00911158_0, 1, 1;
    %load/v 8, v009110A8_0, 8;
    %movi 16, 0, 11;
    %muli 8, 1000, 19;
    %ix/get 0, 8, 19;
    %delayx 0;
    %wait E_001F9F60;
    %set/v v00911158_0, 0, 1;
    %end;
S_001F8698 .scope task, "change_seed" "change_seed" 2 82, 2 82, S_001F8610;
 .timescale -9 -12;
v00911260_0 .var "seed_value", 7 0;
TD_lfsr_galois_tb1.change_seed ;
    %load/v 8, v00911260_0, 8;
    %set/v v00910EF0_0, 8, 8;
    %delay 10000, 0;
    %end;
S_001F8E90 .scope task, "random_valid" "random_valid" 2 90, 2 90, S_001F8610;
 .timescale -9 -12;
TD_lfsr_galois_tb1.random_valid ;
    %set/v v009114C8_0, 0, 32;
T_2.0 ;
    %load/v 8, v009114C8_0, 32;
   %cmpi/s 8, 100, 32;
    %jmp/0xz T_2.1, 5;
    %wait E_001F9F60;
    %vpi_func 2 94 "$random", 8, 32;
    %movi 40, 2, 32;
    %mod/s 8, 40, 32;
    %set/v v009112B8_0, 8, 1;
    %delay 100000, 0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v009114C8_0, 32;
    %set/v v009114C8_0, 8, 32;
    %jmp T_2.0;
T_2.1 ;
    %end;
S_001F9358 .scope task, "soft_reset" "soft_reset" 2 70, 2 70, S_001F8610;
 .timescale -9 -12;
v009113C0_0 .var "random_soft_reset", 7 0;
E_001F9F60 .event posedge, v009124E8_0;
TD_lfsr_galois_tb1.soft_reset ;
    %vpi_func 2 73 "$urandom_range", 8, 32, 2'sb01, 12'sb011111010000;
    %ix/load 0, 8, 0;
    %assign/v0 v009113C0_0, 0, 8;
