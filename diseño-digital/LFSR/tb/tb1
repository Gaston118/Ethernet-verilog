    %load/v 8, v009113C0_0, 8;
    %movi 16, 0, 11;
    %muli 8, 1000, 19;
    %ix/get 0, 8, 19;
    %delayx 0;
    %wait E_001F9F60;
    %set/v v00910FA0_0, 0, 1;
    %end;
S_001F8A50 .scope module, "dut" "lfsr_galois" 2 18, 3 1, S_001F8610;
 .timescale -9 -12;
