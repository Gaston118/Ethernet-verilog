#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_012A9160 .scope module, "lfsr_galois_tb2" "lfsr_galois_tb2" 2 9;
 .timescale -9 -12;
v0092C128_0 .var "clk", 0 0;
v0092C288_0 .var "cycle_count", 31 0;
v0092C230_0 .var/i "i", 31 0;
v0092C180_0 .var "i_rst", 0 0;
v0092BD60_0 .var "i_seed", 7 0;
v0092C020_0 .var "i_soft_reset", 0 0;
v0092C1D8_0 .var "i_valid", 0 0;
v0092C0D0_0 .var "initial_value", 7 0;
v0092BF70_0 .net "o_lfsr", 7 0, L_012A9858; 1 drivers
v0092C338_0 .var "repeated", 0 0;
S_012A8B00 .scope task, "async_reset" "async_reset" 2 58, 2 58, S_012A9160;
 .timescale -9 -12;
v0092BCB0_0 .var "random_reset", 7 0;
TD_lfsr_galois_tb2.async_reset ;
    %vpi_func 2 61 "$urandom_range", 8, 32, 2'sb01, 12'sb011111010000;
    %ix/load 0, 8, 0;
    %assign/v0 v0092BCB0_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 63 "$display", "Numero: %0d", v0092BCB0_0;
    %set/v v0092C180_0, 1, 1;
    %load/v 8, v0092BCB0_0, 8;
    %movi 16, 0, 11;
    %muli 8, 1000, 19;
    %ix/get 0, 8, 19;
    %delayx 0;
    %wait E_008F2848;
    %set/v v0092C180_0, 0, 1;
    %end;
S_012A8968 .scope task, "change_seed" "change_seed" 2 82, 2 82, S_012A9160;
 .timescale -9 -12;
v0092BE10_0 .var "seed_value", 7 0;
TD_lfsr_galois_tb2.change_seed ;
    %load/v 8, v0092BE10_0, 8;
    %set/v v0092BD60_0, 8, 8;
    %delay 10000, 0;
    %end;
S_012A8390 .scope task, "check_period" "check_period" 2 104, 2 104, S_012A9160;
 .timescale -9 -12;
TD_lfsr_galois_tb2.check_period ;
    %set/v v0092C1D8_0, 1, 1;
    %set/v v0092C338_0, 0, 1;
    %delay 50000, 0;
    %load/v 8, v0092BF70_0, 8;
    %set/v v0092C0D0_0, 8, 8;
    %set/v v0092C288_0, 0, 32;
T_2.0 ;
    %load/v 8, v0092C338_0, 1;
    %inv 8, 1;
    %jmp/0xz T_2.1, 8;
    %wait E_008F2848;
    %load/v 8, v0092C288_0, 32;
    %mov 40, 0, 1;
    %addi 8, 1, 33;
    %set/v v0092C288_0, 8, 32;
    %delay 1000, 0;
    %load/v 8, v0092BF70_0, 8;
    %load/v 16, v0092C0D0_0, 8;
    %cmp/u 8, 16, 8;
    %mov 8, 4, 1;
    %movi 9, 1, 32;
    %load/v 41, v0092C288_0, 32;
    %cmp/u 9, 41, 32;
    %mov 9, 5, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_2.2, 8;
    %set/v v0092C338_0, 1, 1;
    %set/v v0092C1D8_0, 0, 1;
T_2.2 ;
    %jmp T_2.0;
T_2.1 ;
    %load/v 8, v0092C338_0, 1;
    %jmp/0xz  T_2.4, 8;
    %vpi_call 2 123 "$display", "LFSR repeated after %0d cycles.", v0092C288_0;
T_2.4 ;
    %end;
S_012A8308 .scope task, "random_valid" "random_valid" 2 90, 2 90, S_012A9160;
 .timescale -9 -12;
TD_lfsr_galois_tb2.random_valid ;
    %set/v v0092C230_0, 0, 32;
T_3.6 ;
    %load/v 8, v0092C230_0, 32;
   %cmpi/s 8, 100, 32;
    %jmp/0xz T_3.7, 5;
    %wait E_008F2848;
    %vpi_func 2 94 "$random", 8, 32;
    %movi 40, 2, 32;
    %mod/s 8, 40, 32;
    %set/v v0092C1D8_0, 8, 1;
    %delay 100000, 0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0092C230_0, 32;
    %set/v v0092C230_0, 8, 32;
    %jmp T_3.6;
T_3.7 ;
    %end;
S_012A87D0 .scope task, "soft_reset" "soft_reset" 2 70, 2 70, S_012A9160;
 .timescale -9 -12;
v0092BEC0_0 .var "random_soft_reset", 7 0;
E_008F2848 .event posedge, v008F4670_0;
TD_lfsr_galois_tb2.soft_reset ;
    %vpi_func 2 73 "$urandom_range", 8, 32, 2'sb01, 12'sb011111010000;
    %ix/load 0, 8, 0;
    %assign/v0 v0092BEC0_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 75 "$display", "Numero: %0d", v0092BEC0_0;
    %set/v v0092C020_0, 1, 1;
    %load/v 8, v0092BEC0_0, 8;
    %movi 16, 0, 11;
    %muli 8, 1000, 19;
    %ix/get 0, 8, 19;
    %delayx 0;
    %wait E_008F2848;
    %set/v v0092C020_0, 0, 1;
    %end;
S_012A86C0 .scope module, "dut" "lfsr_galois" 2 18, 3 1, S_012A9160;
 .timescale -9 -12;
L_012A97E8 .functor XOR 1, L_0092C078, L_0092C2E0, C4<0>, C4<0>;
L_012A9858 .functor BUFZ 8, v012A3248_0, C4<00000000>, C4<00000000>, C4<00000000>;
v012A9B50_0 .net *"_s1", 0 0, L_0092C078; 1 drivers
v012AA628_0 .net *"_s3", 6 0, L_0092BDB8; 1 drivers
v012AAB50_0 .net *"_s4", 6 0, C4<0000000>; 1 drivers
v008F44F0_0 .net *"_s6", 0 0, L_0092C2E0; 1 drivers
v008F4670_0 .net "clk", 0 0, v0092C128_0; 1 drivers
v008F46C8_0 .net "feedback", 0 0, L_012A97E8; 1 drivers
v008F4720_0 .net "i_rst", 0 0, v0092C180_0; 1 drivers
v00902490_0 .net "i_seed", 7 0, v0092BD60_0; 1 drivers
v009024E8_0 .net "i_soft_reset", 0 0, v0092C020_0; 1 drivers
v00902540_0 .net "i_valid", 0 0, v0092C1D8_0; 1 drivers
v012A3248_0 .var "lfsr_reg", 7 0;
v012A32A0_0 .alias "o_lfsr", 7 0, v0092BF70_0;
v012A32F8_0 .var "seed_reg", 7 0;
E_008F2728 .event posedge, v008F4720_0, v008F4670_0;
L_0092C078 .part v012A3248_0, 7, 1;
L_0092BDB8 .part v012A3248_0, 0, 7;
L_0092C2E0 .cmp/eq 7, L_0092BDB8, C4<0000000>;
    .scope S_012A86C0;
T_5 ;
    %movi 8, 1, 8;
    %set/v v012A32F8_0, 8, 8;
    %end;
    .thread T_5;
    .scope S_012A86C0;
T_6 ;
    %wait E_008F2728;
    %load/v 8, v008F4720_0, 1;
    %jmp/0xz  T_6.0, 8;
    %load/v 8, v012A32F8_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v012A3248_0, 0, 8;
    %jmp T_6.1;
T_6.0 ;
    %load/v 8, v009024E8_0, 1;
    %jmp/0xz  T_6.2, 8;
    %load/v 8, v00902490_0, 8;
    %ix/load 0, 8, 0;
    %assign/v0 v012A3248_0, 0, 8;
    %jmp T_6.3;
T_6.2 ;
    %load/v 8, v00902540_0, 1;
    %jmp/0xz  T_6.4, 8;
    %load/v 8, v008F46C8_0, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 0, 0;
    %assign/v0/x1 v012A3248_0, 0, 8;
    %load/v 8, v012A3248_0, 1; Only need 1 of 8 bits
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %ix/load 1, 1, 0;
    %assign/v0/x1 v012A3248_0, 0, 8;
    %ix/load 1, 1, 0;
    %mov 4, 0, 1;
    %jmp/1 T_6.6, 4;
    %load/x1p 8, v012A3248_0, 1;
    %jmp T_6.7;
T_6.6 ;
    %mov 8, 2, 1;
T_6.7 ;
; Save base=8 wid=1 in lookaside.
    %load/v 9, v008F46C8_0, 1;
    %xor 8, 9, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 2, 0;
    %assign/v0/x1 v012A3248_0, 0, 8;
    %ix/load 1, 2, 0;
    %mov 4, 0, 1;
    %jmp/1 T_6.8, 4;
    %load/x1p 8, v012A3248_0, 1;
    %jmp T_6.9;
T_6.8 ;
    %mov 8, 2, 1;
T_6.9 ;
; Save base=8 wid=1 in lookaside.
    %load/v 9, v008F46C8_0, 1;
    %xor 8, 9, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 3, 0;
    %assign/v0/x1 v012A3248_0, 0, 8;
    %ix/load 1, 3, 0;
    %mov 4, 0, 1;
    %jmp/1 T_6.10, 4;
    %load/x1p 8, v012A3248_0, 1;
    %jmp T_6.11;
T_6.10 ;
    %mov 8, 2, 1;
T_6.11 ;
; Save base=8 wid=1 in lookaside.
    %load/v 9, v008F46C8_0, 1;
    %xor 8, 9, 1;
    %ix/load 0, 1, 0;
    %ix/load 1, 4, 0;
    %assign/v0/x1 v012A3248_0, 0, 8;
    %ix/load 1, 4, 0;
    %mov 4, 0, 1;
    %jmp/1 T_6.12, 4;
    %load/x1p 8, v012A3248_0, 1;
    %jmp T_6.13;
T_6.12 ;
    %mov 8, 2, 1;
T_6.13 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %ix/load 1, 5, 0;
    %assign/v0/x1 v012A3248_0, 0, 8;
    %ix/load 1, 5, 0;
    %mov 4, 0, 1;
    %jmp/1 T_6.14, 4;
    %load/x1p 8, v012A3248_0, 1;
    %jmp T_6.15;
T_6.14 ;
    %mov 8, 2, 1;
T_6.15 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %ix/load 1, 6, 0;
    %assign/v0/x1 v012A3248_0, 0, 8;
    %ix/load 1, 6, 0;
    %mov 4, 0, 1;
    %jmp/1 T_6.16, 4;
    %load/x1p 8, v012A3248_0, 1;
    %jmp T_6.17;
T_6.16 ;
    %mov 8, 2, 1;
T_6.17 ;
; Save base=8 wid=1 in lookaside.
    %ix/load 0, 1, 0;
    %ix/load 1, 7, 0;
    %assign/v0/x1 v012A3248_0, 0, 8;
T_6.4 ;
T_6.3 ;
T_6.1 ;
    %jmp T_6;
    .thread T_6;
    .scope S_012A9160;
T_7 ;
    %delay 50000, 0;
    %load/v 8, v0092C128_0, 1;
    %inv 8, 1;
    %set/v v0092C128_0, 8, 1;
    %jmp T_7;
    .thread T_7;
    .scope S_012A9160;
T_8 ;
    %vpi_call 2 32 "$dumpfile", "tb/lfsr_galois_tb2.vcd";
    %vpi_call 2 33 "$dumpvars", 1'sb0, S_012A9160;
    %set/v v0092C128_0, 0, 1;
    %set/v v0092C1D8_0, 0, 1;
    %set/v v0092C180_0, 0, 1;
    %set/v v0092C020_0, 0, 1;
    %set/v v0092BD60_0, 0, 8;
    %fork TD_lfsr_galois_tb2.async_reset, S_012A8B00;
    %join;
    %delay 1000000, 0;
    %set/v v0092BE10_0, 0, 8;
    %fork TD_lfsr_galois_tb2.change_seed, S_012A8968;
    %join;
    %fork TD_lfsr_galois_tb2.soft_reset, S_012A87D0;
    %join;
    %delay 1000000, 0;
    %fork TD_lfsr_galois_tb2.check_period, S_012A8390;
    %join;
    %delay 1000000, 0;
    %vpi_call 2 55 "$finish";
    %end;
    .thread T_8;
