S_008885A0 .scope module, "lfsr_galois_tb2" "lfsr_galois_tb2" 2 9;
 .timescale -9 -12;
v0119C328_0 .var "clk", 0 0;
v0119BEB0_0 .var "cycle_count", 31 0;
v0119C2D0_0 .var/i "i", 31 0;
v0119BCA0_0 .var "i_rst", 0 0;
v0119C220_0 .var "i_seed", 7 0;
v0119BD50_0 .var "i_soft_reset", 0 0;
