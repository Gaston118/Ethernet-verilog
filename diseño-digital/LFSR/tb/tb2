v0119C380_0 .var "repeated", 0 0;
S_008887C0 .scope task, "async_reset" "async_reset" 2 58, 2 58, S_008885A0;
 .timescale -9 -12;
v0119C010_0 .var "random_reset", 7 0;
TD_lfsr_galois_tb2.async_reset ;
    %vpi_func 2 61 "$urandom_range", 8, 32, 2'sb01, 12'sb011111010000;
    %ix/load 0, 8, 0;
    %assign/v0 v0119C010_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 63 "$display", "Numero: %0d", v0119C010_0;
    %set/v v0119BCA0_0, 1, 1;
    %load/v 8, v0119C010_0, 8;
    %movi 16, 0, 11;
    %muli 8, 1000, 19;
    %ix/get 0, 8, 19;
    %delayx 0;
    %wait E_011620A8;
    %set/v v0119BCA0_0, 0, 1;
    %end;
S_008886B0 .scope task, "change_seed" "change_seed" 2 82, 2 82, S_008885A0;
 .timescale -9 -12;
v0119BFB8_0 .var "seed_value", 7 0;
TD_lfsr_galois_tb2.change_seed ;
    %load/v 8, v0119BFB8_0, 8;
    %set/v v0119C220_0, 8, 8;
    %delay 10000, 0;
    %end;
S_00888628 .scope task, "check_period" "check_period" 2 104, 2 104, S_008885A0;
 .timescale -9 -12;
TD_lfsr_galois_tb2.check_period ;
    %set/v v0119C068_0, 1, 1;
    %set/v v0119C380_0, 0, 1;
    %delay 50000, 0;
    %load/v 8, v0119BDA8_0, 8;
    %set/v v0119C118_0, 8, 8;
    %delay 100000, 0;
    %set/v v0119BEB0_0, 0, 32;
T_2.0 ;
    %load/v 8, v0119C380_0, 1;
    %inv 8, 1;
    %jmp/0xz T_2.1, 8;
    %wait E_011620A8;
    %load/v 8, v0119BEB0_0, 32;
    %mov 40, 0, 1;
    %addi 8, 1, 33;
    %set/v v0119BEB0_0, 8, 32;
    %load/v 8, v0119BDA8_0, 8;
    %load/v 16, v0119C118_0, 8;
    %cmp/u 8, 16, 8;
    %mov 8, 4, 1;
    %movi 9, 1, 32;
    %load/v 41, v0119BEB0_0, 32;
    %cmp/u 9, 41, 32;
    %mov 9, 5, 1;
    %and 8, 9, 1;
    %jmp/0xz  T_2.2, 8;
    %set/v v0119C380_0, 1, 1;
    %set/v v0119C068_0, 0, 1;
T_2.2 ;
    %jmp T_2.0;
T_2.1 ;
    %load/v 8, v0119C380_0, 1;
    %jmp/0xz  T_2.4, 8;
    %vpi_call 2 123 "$display", "LFSR sequence repeated after %0d cycles.", v0119BEB0_0;
    %jmp T_2.5;
T_2.4 ;
    %vpi_call 2 125 "$display", "LFSR did not repeat.";
T_2.5 ;
    %end;
S_00888958 .scope task, "random_valid" "random_valid" 2 90, 2 90, S_008885A0;
 .timescale -9 -12;
TD_lfsr_galois_tb2.random_valid ;
    %set/v v0119C2D0_0, 0, 32;
T_3.6 ;
    %load/v 8, v0119C2D0_0, 32;
   %cmpi/s 8, 100, 32;
    %jmp/0xz T_3.7, 5;
    %wait E_011620A8;
    %vpi_func 2 94 "$random", 8, 32;
    %movi 40, 2, 32;
    %mod/s 8, 40, 32;
    %set/v v0119C068_0, 8, 1;
    %delay 100000, 0;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0119C2D0_0, 32;
    %set/v v0119C2D0_0, 8, 32;
    %jmp T_3.6;
T_3.7 ;
    %end;
S_00888518 .scope task, "soft_reset" "soft_reset" 2 70, 2 70, S_008885A0;
 .timescale -9 -12;
v0119BE00_0 .var "random_soft_reset", 7 0;
E_011620A8 .event posedge, v0088AC70_0;
TD_lfsr_galois_tb2.soft_reset ;
    %vpi_func 2 73 "$urandom_range", 8, 32, 2'sb01, 12'sb011111010000;
    %ix/load 0, 8, 0;
    %assign/v0 v0119BE00_0, 0, 8;
    %delay 10000, 0;
    %vpi_call 2 75 "$display", "Numero: %0d", v0119BE00_0;
    %set/v v0119BD50_0, 1, 1;
    %load/v 8, v0119BE00_0, 8;
    %movi 16, 0, 11;
    %muli 8, 1000, 19;
    %ix/get 0, 8, 19;
    %delayx 0;
    %wait E_011620A8;
    %set/v v0119BD50_0, 0, 1;
    %end;
S_00888490 .scope module, "dut" "lfsr_galois" 2 18, 3 1, S_008885A0;
 .timescale -9 -12;
