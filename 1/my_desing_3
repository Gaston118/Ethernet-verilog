#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_008C4448 .scope module, "first_counter_tb" "first_counter_tb" 2 3;
 .timescale 0 0;
v00993968_0 .var "clock", 0 0;
v008CECD0_0 .net "counter_out", 3 0, v00993860_0; 1 drivers
v008CED28_0 .var "enable", 0 0;
v008CED80_0 .var "reset", 0 0;
S_008C31D0 .scope module, "U_counter" "first_counter" 2 31, 3 10, S_008C4448;
 .timescale 0 0;
v00993808_0 .net "clock", 0 0, v00993968_0; 1 drivers
v00993860_0 .var "counter_out", 3 0;
v009938B8_0 .net "enable", 0 0, v008CED28_0; 1 drivers
v00993910_0 .net "reset", 0 0, v008CED80_0; 1 drivers
E_009717F0 .event posedge, v00993808_0;
S_008C3258 .scope begin, "COUNTER" "COUNTER" 3 27, 3 27, S_008C31D0;
 .timescale 0 0;
    .scope S_008C31D0;
T_0 ;
    %wait E_009717F0;
    %fork t_1, S_008C3258;
    %jmp t_0;
    .scope S_008C3258;
t_1 ;
    %load/v 8, v00993910_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v00993860_0, 1, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v009938B8_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v00993860_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v00993860_0, 1, 8;
T_0.2 ;
T_0.1 ;
    %end;
    .scope S_008C31D0;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_008C4448;
T_1 ;
    %vpi_call 2 9 "$dumpfile", "first_counter_tb.vcd";
    %vpi_call 2 10 "$dumpvars", 1'sb0, S_008C4448;
    %vpi_call 2 11 "$display", "time\011 clk reset enable counter";
    %vpi_call 2 12 "$monitor", "%g\011 %b   %b     %b      %b", $time, v00993968_0, v008CED80_0, v008CED28_0, v008CECD0_0;
    %set/v v00993968_0, 1, 1;
    %set/v v008CED80_0, 0, 1;
    %set/v v008CED28_0, 0, 1;
    %delay 5, 0;
    %set/v v008CED80_0, 1, 1;
    %delay 10, 0;
    %set/v v008CED80_0, 0, 1;
    %delay 10, 0;
    %set/v v008CED28_0, 1, 1;
    %delay 100, 0;
    %set/v v008CED28_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 23 "$finish";
    %end;
    .thread T_1;
    .scope S_008C4448;
T_2 ;
    %delay 5, 0;
    %load/v 8, v00993968_0, 1;
    %inv 8, 1;
    %set/v v00993968_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "first_counter_tb.v";
    "first_counter.v";
