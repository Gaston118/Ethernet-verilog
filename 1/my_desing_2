:vpi_module "v2005_math";
:vpi_module "va_math";
S_01174380 .scope module, "first_counter_tb" "first_counter_tb" 2 2;
 .timescale 0 0;
v0117ECC8_0 .var "clock", 0 0;
v0117ED20_0 .net "counter_out", 3 0, v00FB39C8_0; 1 drivers
v0117ED78_0 .var "enable", 0 0;
v0117EDD0_0 .var "reset", 0 0;
S_01173158 .scope module, "U_counter" "first_counter" 2 27, 3 10, S_01174380;
 .timescale 0 0;
v00FB3F90_0 .net "clock", 0 0, v0117ECC8_0; 1 drivers
v00FB39C8_0 .var "counter_out", 3 0;
v0117EC18_0 .net "enable", 0 0, v0117ED78_0; 1 drivers
v0117EC70_0 .net "reset", 0 0, v0117EDD0_0; 1 drivers
E_00FB1E10 .event posedge, v00FB3F90_0;
S_011731E0 .scope begin, "COUNTER" "COUNTER" 3 27, 3 27, S_01173158;
 .timescale 0 0;
    .scope S_01173158;
T_0 ;
    %wait E_00FB1E10;
    %fork t_1, S_011731E0;
    %jmp t_0;
    .scope S_011731E0;
t_1 ;
    %load/v 8, v0117EC70_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v00FB39C8_0, 1, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/v 8, v0117EC18_0, 1;
    %jmp/0xz  T_0.2, 8;
    %load/v 8, v00FB39C8_0, 4;
    %mov 12, 0, 28;
    %addi 8, 1, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v00FB39C8_0, 1, 8;
T_0.2 ;
T_0.1 ;
    %end;
    .scope S_01173158;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_01174380;
T_1 ;
    %vpi_call 2 8 "$display", "time\011 clk reset enable counter";
    %vpi_call 2 9 "$monitor", "%g\011 %b   %b     %b      %b", $time, v0117ECC8_0, v0117EDD0_0, v0117ED78_0, v0117ED20_0;
    %set/v v0117ECC8_0, 1, 1;
    %set/v v0117EDD0_0, 0, 1;
    %set/v v0117ED78_0, 0, 1;
    %delay 5, 0;
    %set/v v0117EDD0_0, 1, 1;
    %delay 10, 0;
    %set/v v0117EDD0_0, 0, 1;
    %delay 10, 0;
    %set/v v0117ED78_0, 1, 1;
    %delay 100, 0;
    %set/v v0117ED78_0, 0, 1;
    %delay 5, 0;
    %vpi_call 2 20 "$finish";
    %end;
    .thread T_1;
    .scope S_01174380;
T_2 ;
    %delay 5, 0;
    %load/v 8, v0117ECC8_0, 1;
    %inv 8, 1;
    %set/v v0117ECC8_0, 8, 1;
    %jmp T_2;
    .thread T_2;
